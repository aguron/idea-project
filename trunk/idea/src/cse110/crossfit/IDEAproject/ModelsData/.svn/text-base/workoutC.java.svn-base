package cse110.crossfit.IDEAproject.ModelsData;

//workout table constructor
public class workoutC {
	int workout_id;
	int wod_id;
	String[] workout_text;
	String workout_name;
	String workout_description;
	int timed;
	int AMRAP;
	long timeLimit;
	//empty
	public workoutC(){
		
	}
	
	public workoutC(int workout_id, int wod_id, String workout_name, String workout_description, int timed, int AMRAP, long timeLimit){
		this.workout_id = workout_id;
		this.wod_id = wod_id;
		this.workout_name = workout_name;
		this.workout_description = workout_description;
		this.timed = timed;
		this.AMRAP = AMRAP;
		this.timeLimit = timeLimit;
	}
	
	public workoutC(int wod_id, String workout_name, String workout_description , int timed, int AMRAP, long timeLimit){
		this.wod_id = wod_id;
		this.workout_name = workout_name;
		this.workout_description = workout_description;
		this.timed = timed;
		this.AMRAP = AMRAP; 
		this.timeLimit = timeLimit;
	}
	
	 
    // getting workoutID
    public int getWorkoutID(){
        return this.workout_id;
    }
 
    // setting workoutID
    public void setWorkoutID(int workout_id){
        this.workout_id = workout_id;
    } 
    
    //getting WODID
    public int getWODID(){
    	return this.wod_id;
    }
    
    //setting WODID
    public void setWODID(int wod_id){
    	this.wod_id = wod_id;
    }
  
  //getting WorkoutName
    public String getWorkoutName(){
    	return this.workout_name;
    }
    
    //setting WorkoutName
    public void setWorkoutName(String workout_name){
    	this.workout_name = workout_name;
    }
    
    //getting WorkoutDescription
    public String getWorkoutDescription(){
    	return this.workout_description;
    }
    
    //setting WorkoutDescription
    public void setWorkoutDescription(String workout_description){
    	this.workout_description = workout_description;
    }
    
    // setting time
    public void setTimed(int timed){
    	this.timed = timed;
    }
    
    // getting time
    public int getTime(){
    	return this.timed;
    }
    
    //setting AMRAP
    public void setAMRAP(int AMRAP){
    	this.AMRAP = AMRAP;
    }
    //getting AMRAP
    public int getAMRAP(){
    	return this.AMRAP;
    }
    
    //setting workout_text
    public void setWorkOutText(String[] workout_text )
    {
    	this.workout_text = workout_text;
    }
    
    //get workout text
    public String[] getWorkOutText(){
    	return this.workout_text;
    }
    
    //setting workout timelimt
    public void setWorkoutTimeLimit(long timeLimit){
    	this.timeLimit = timeLimit;
    }
    
    //getting workout timelimit
    public long getWorkoutTimeLimit(){
    	return this.timeLimit;
    }
    
}
